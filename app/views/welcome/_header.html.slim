/ .full-page-width
.row.full-width
  nav.top-bar data-topbar="" role="navigation"
    section.top-bar-section
      ul.left
        li.name style='background-color: #008CBA'
          a href="/" = t('title')
        li.name style='background-color: red'
          a href=t('view.fourth_topic.href') = 'â˜° ' + t('view.fourth_topic.button')
          / a href= available_surveys_url = t('view.top_nav.first')
        li.name style='background-color: green'
          a href=t('view.5_topic.href') = t('view.5_topic.button')
        li.name style='background-color: #008CBA'
          a href='/dashboard/index' = t('view.6_topic.button')
        / li.has-dropdown
        /   a href="#"  Right Button Dropdown
        /   ul.dropdown
        /     li
        /       a href="#"  First link in dropdown
        /     li.active
        /       a href="#"  Active link in dropdown
      ul.right
        li.name style='background-color: #008CBA'
          -if user_signed_in?
            a href="/sign_out" = t('devise.sign_out')
          -else
            a href="/sign_in" = t('devise.sign_in')

